{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/ai_proxy/server.js"
    }
  ],
  "builds": [
    {
      "src": "ai_proxy/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { 
        "src": "/", 
        "dest": "/main.html" 
    },
    { 
        "src": "/main.html", 
        "dest": "/main.html" 
    },
    {
        "src": "/(.*)",
        "dest": "/$1"
    }
  ]
}
